<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageButton
        android:id="@+id/wall_paper_item"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageButton
        android:id="@+id/love_item"
        android:layout_width="@dimen/love_dimen"
        android:layout_height="@dimen/love_dimen"
        android:src="@drawable/love_list_level"
        android:layout_marginLeft="@dimen/love_margin"
        android:layout_marginTop="@dimen/love_margin"/>

</FrameLayout>